(* booleans, and, or, not, if statement *)

val operator_if : Bool -> 'd -> 'd -> 'e
let operator_if c t e = _if c _then t _else e

val f: 'a -> 'a
let f = (\x -> x)

let x = true
let tru = f (x || false)
let fls = f (x && (! true))
let main = if tru 
           then
              if fls 
              then 0
              else 1
           else 0

